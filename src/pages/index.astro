<script>
(() => {
    const hero = document.querySelector("section[data-behavior='home-hero']");
    if (!hero) return;

    let tx = 56;
    let ty = 60;
    let cx = 56;
    let cy = 60;
    let dx = 0;
    let dy = 0;
    let tilt = -8;
    let raf = 0;

    const animate = () => {
        cx += (tx - cx) * 0.2;
        cy += (ty - cy) * 0.2;
        dx += ((tx - 56) * 1.3 - dx) * 0.16;
        dy += ((ty - 60) * 1.1 - dy) * 0.16;
        tilt += (((tx - 56) * 0.22 - 8) - tilt) * 0.12;
        hero.style.setProperty("--hero-cx", `${cx}%`);
        hero.style.setProperty("--hero-cy", `${cy}%`);
        hero.style.setProperty("--hero-dx", `${dx}px`);
        hero.style.setProperty("--hero-dy", `${dy}px`);
        hero.style.setProperty("--hero-tilt", `${tilt}deg`);
        raf = window.requestAnimationFrame(animate);
    };

    const updateFromEvent = (event) => {
        const rect = hero.getBoundingClientRect();
        if (!rect.width || !rect.height) return;
        const nx = (event.clientX - rect.left) / rect.width;
        const ny = (event.clientY - rect.top) / rect.height;
        tx = Math.max(26, Math.min(74, nx * 100));
        ty = Math.max(34, Math.min(86, ny * 100));
    };

    hero.addEventListener("pointermove", updateFromEvent);
    hero.addEventListener("pointerleave", () => {
        tx = 56;
        ty = 60;
    });

    animate();
    window.addEventListener("beforeunload", () => window.cancelAnimationFrame(raf), { once: true });
})();
</script>
<script>
(() => {
  const lazyVideos = Array.from(document.querySelectorAll("video[preload='none']"));
  if (!lazyVideos.length || !("IntersectionObserver" in window)) return;
  const io = new IntersectionObserver((entries) => {
    for (const entry of entries) {
      if (!entry.isIntersecting) continue;
      const video = entry.target;
      if (video.dataset.loaded === "true") continue;
      video.dataset.loaded = "true";
      video.load();
      io.unobserve(video);
    }
  }, { rootMargin: "200px 0px" });
  lazyVideos.forEach((video) => io.observe(video));
})();
</script>

